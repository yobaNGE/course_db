<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.course.viewTable.ProviderController">
    <children>
        <TableView fx:id="provider_table" layoutX="197.0" layoutY="54.0" prefHeight="333.0" prefWidth="389.0">
            <columns>
                <TableColumn fx:id="id" prefWidth="74.0" text="id" />
                <TableColumn fx:id="amountofdebt" prefWidth="91.0" text="amountofdebt" />
                <TableColumn fx:id="adress" minWidth="0.0" prefWidth="107.0" text="adress" />
                <TableColumn fx:id="name" prefWidth="116.0" text="name" />
            </columns>
        </TableView>
        <Button fx:id="provider_button" layoutX="48.0" layoutY="84.0" mnemonicParsing="false" onAction="#provider" text="Update table!" />
      <Label fx:id="id_tf" contentDisplay="CENTER" layoutX="61.0" layoutY="109.0" prefHeight="9.0" prefWidth="114.0" />
      <TextField fx:id="amount_tf" layoutX="23.0" layoutY="146.0" prefHeight="25.0" prefWidth="149.0" promptText="Колво">
         <opaqueInsets>
            <Insets />
         </opaqueInsets></TextField>
      <TextField fx:id="adress_tf" layoutX="22.0" layoutY="187.0" prefHeight="25.0" prefWidth="149.0" promptText="Адрес" />
      <TextField fx:id="name_tf" layoutX="22.0" layoutY="231.0" promptText="Имя" />
      <Button layoutX="66.0" layoutY="309.0" mnemonicParsing="false" onAction="#edit" prefHeight="17.0" prefWidth="51.0" text="Save" />
      <Button layoutX="22.0" layoutY="309.0" mnemonicParsing="false" onAction="#generateNew" text="New" />
      <Button layoutX="124.0" layoutY="309.0" mnemonicParsing="false" onAction="#delete" text="Delete" />
      <Button fx:id="back" layoutX="66.0" layoutY="352.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
    </children>
</AnchorPane>
